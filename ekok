def ekokk(sayi1,sayi2):
    ekok = sayi1*sayi2
    for i in range(ekok,max(sayi1,sayi2)-1,-1):
        if i % sayi1 == 0 and i % sayi2 == 0:
            ekok = i
    return ekok 
       
GirSayi1 = int(input("1.Say覺y覺 girin: "))
GirSayi2 = int(input("2.Say覺y覺 girin: "))
print(ekokk(GirSayi1, GirSayi2))
